
<article class="post">
  <div class="title">
    {{user.username}} 
  <a href="{{post_url}}" target="_blank">{{title}}</a>
  <span>({{format_url post_url}})</span>
</div>
  <div class="meta">
    <a href="/post/{{id}}">{{vote_count}} <img src="/images/greenHeart.png" width="25" height="25"></a>
    
    <a href="/post/{{id}}">{{comments.length}} <img src="/images/commentIcon.png" width="25" height="25"></a>
  </div>
</article>
